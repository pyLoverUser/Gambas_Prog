Select Case Message.Question("Soll die Auto - Beregnung,<br/>BEGINN um <font color='red'>< b >" & GridViewAuto1[row, 2].Text & "</b></font> gelöscht werden?", "Ja", "Nein")
Case 1
SqlCom = "DELETE From "
SqlTab = "r_RegnerAuto "
SqlP = " WHERE id = " & i
rs = DatabaseService.ExecuteSql(SqlCom, SqlTab, SqlP)
Case 2
End Select
