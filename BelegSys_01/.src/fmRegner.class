Public Sub grMan_Change()
Dim i As Integer
Dim k As String 
If akFl = False Then Return   
i = Last.Tag
btnAdmin[4].Enabled = True
Select Case i
    Case 1
        k = comRegMan1.Text
    Case 2
        k = comRegMan2.Text
   Case 3
        If comRegMan3.Text = "N" Then
            frRegMan2.Visible = True
